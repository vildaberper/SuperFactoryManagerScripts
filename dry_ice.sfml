name "DryIce"
every 20 ticks do
    if energize has >=1 item:: then
        input from energize bottom side
        output to controller
        forget
    end

    if energize has <= 0 item:: then
        if controller has >=2 blue_ice and controller has <10000 powah:dry_ice then
            input 2 blue_ice from controller
            output to energize
        end
    end
end
